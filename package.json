{
  "name": "@junto-foundation/junto-elements",
  "version": "0.0.25",
  "description": "",
  "homepage": "https://junto-elements.netlify.com",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "dependencies": {
    "@holo-host/identicon": "^0.1.0",
    "@popperjs/core": "^2.9.2",
    "@tiptap/core": "^2.0.0-beta.66",
    "@tiptap/extension-bold": "^2.0.0-beta.13",
    "@tiptap/extension-code-block": "^2.0.0-beta.15",
    "@tiptap/extension-document": "^2.0.0-beta.12",
    "@tiptap/extension-paragraph": "^2.0.0-beta.14",
    "@tiptap/extension-placeholder": "^2.0.0-beta.19",
    "@tiptap/extension-text": "^2.0.0-beta.12",
    "@tiptap/starter-kit": "^2.0.0-beta.63",
    "lit": "^2.0.0-rc.2",
    "prismjs": "^1.23.0"
  },
  "devDependencies": {
    "esbuild": "^0.12.0",
    "highlight.js": "^10.7.2",
    "live-server": "^1.2.1",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "web-component-analyzer": "^1.1.6"
  },
  "scripts": {
    "prepare": "run-s build:*",
    "release": "np --no-2fa --no-tests",
    "start": "run-p dev:*",
    "build": "run-s build:*",
    "dev": "run-p dev:*",
    "dev:docs": "live-server",
    "dev:watch": "esbuild src/main.ts --watch --bundle --minify --sourcemap --outfile=dist/main.js --target=es2016",
    "build:analyze": "web-component-analyzer src --outFile components.json",
    "build:package": "esbuild src/main.ts --metafile=meta.json --bundle --minify --sourcemap --outfile=dist/main.js --target=es2016"
  },
